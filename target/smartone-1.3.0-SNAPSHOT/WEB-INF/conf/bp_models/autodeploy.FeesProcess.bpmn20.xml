      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="manualtask1" id="BPMNShape_manualtask1">
        <omgdc:Bounds height="55.0" width="105.0" x="660.0" y="150.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="manualtask2" id="BPMNShape_manualtask2">
